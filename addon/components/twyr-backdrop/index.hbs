<md-backdrop
	class="md-default-theme"
	style={{this.backdropPosition}}
	{{has-class-if @isOpaque "md-opaque"}}
	{{has-class-if @isLockedOpen "md-locked-open"}}
	{{css-transition "ng"}}
	{{did-insert this.didInsert}}
	{{on "click" this.handleClick}}
	{{on "touchEnd" this.handleClick}}
	...attributes
>
	{{yield}}
</md-backdrop>
