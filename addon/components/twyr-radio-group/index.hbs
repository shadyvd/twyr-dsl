<md-radio-group
	role={{this.role}}
	aria-labelledby={{this.ariaLabelledBy}}
	{{did-insert this.didInsert}}
	{{is-focusable
		focusOnlyOnKey=this.focusOnlyOnKey
		onMouseEnter=@onMouseEnter
		onMouseLeave=@onMouseLeave
		onMouseDown=@onMouseDown
		onMouseUp=@onMouseUp
		onFocusIn=@onFocusIn
		onFocusOut=@onFocusOut
	}}
	{{on "keydown" this.handleKeydown}}
	...attributes
>
	{{yield (hash
		Label=(component this.subComponents.label
			setAriaLabelledby=this.setAriaLabelledby
		)
		Button=(component this.subComponents.button
			toggle=@toggle
			groupValue=@groupValue
			registerWithParent=this.registerChild
			onChange=this.onChange
		)
	)}}
</md-radio-group>